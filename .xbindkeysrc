"amixer set Master toggle"
  XF86AudioMute

"pamixer -d 5"
  XF86AudioLowerVolume

"pamixer -i 5"
  XF86AudioRaiseVolume

"mpc toggle"
  Mod4 + XF86AudioMute

"mpc prev"
  Mod4 + XF86AudioLowerVolume

"mpc next"
  Mod4 + XF86AudioRaiseVolume

"amixer set Capture toggle"
  F4

"rfkill-toggle'
  F7

"xset dpms force off"
  F9

"systemctl hibernate"
  Mod4 + Delete

"xbacklight -dec 10"
  XF86MonBrightnessDown

"xbacklight -inc 10"
  XF86MonBrightnessUp

"xbacklight -set 2"
  Mod4 + XF86MonBrightnessDown

"xbacklight -set 100"
  Mod4 + XF86MonBrightnessUp

"scrot"
  Print

"scrot -s"
  Mod4 + Print
